<addressConfiguration>
  <wipe>true</wipe>
  <addressComponents>
    <addressComponent>
      <field>COUNTRY</field>
      <nameMapping>Country</nameMapping>
      <sizeMapping>40</sizeMapping>
      <elementDefault>Haiti</elementDefault>
      <requiredInHierarchy>false</requiredInHierarchy>
    </addressComponent>
    <addressComponent>
      <field>STATE_PROVINCE</field>
      <nameMapping>Department</nameMapping>
      <sizeMapping>40</sizeMapping>
