      <field>CITY_VILLAGE</field>
      <nameMapping>Commune</nameMapping>
      <sizeMapping>40</sizeMapping>
      <requiredInHierarchy>false</requiredInHierarchy>
    </addressComponent>
    <addressComponent>
      <field>ADDRESS_3</field>
      <nameMapping>Commune Section</nameMapping>
      <sizeMapping>40</sizeMapping>
      <requiredInHierarchy>false</requiredInHierarchy>
    </addressComponent>
    <addressComponent>
      <field>ADDRESS_1</field>
      <nameMapping>Locality House</nameMapping>
      <sizeMapping>40</sizeMapping>
      <requiredInHierarchy>false</requiredInHierarchy>
    </addressComponent>
    <addressComponent>
      <field>ADDRESS_2</field>
      <nameMapping>Address</nameMapping>
      <sizeMapping>40</sizeMapping>
      <requiredInHierarchy>false</requiredInHierarchy>
    </addressComponent>
